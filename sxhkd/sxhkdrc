super + Return 
    st

XF86Audio{RaiseVolume,LowerVolume}
    pamixer {-i,-d} 2 

XF86Audio{Mic,_}Mute
    pamixer {--default-source,_} -t

XF86MonBrightness{Down,Up}
    xbacklight -{dec,inc} 5


super + p
    rofi -show drun

super + {_,shift + }{1-9,0}
    bspc {desktop -f,node -d} '^{1-9,10}'

super + Escape
    pkill -USR1 -x sxhkd

super + alt + Escape
    bspc quit 1

super + shift + q
	bspc node --close

super + {h,j,k,l}
    bspc node --focus {west,south,north,east}
